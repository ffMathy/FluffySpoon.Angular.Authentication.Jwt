{
	"compilerOptions": {
		"declaration": true,
		"stripInternal": true,
		"experimentalDecorators": true,
		"strictNullChecks": false,
		"noImplicitAny": false,
		"module": "es2015",
		"moduleResolution": "node",
		"rootDir": "./src",
		"outDir": "./dist",
		"sourceMap": true,
		"inlineSources": true,
		"target": "es5",
		"skipLibCheck": true,
		"lib": [
			"es2015",
			"dom"
		]
	},
	"angularCompilerOptions": {
		"strictMetadataEmit": true
	}
}